{
  "nativescript": {
    "id": "com.fitzcreative.envelope",
    "tns-android": {
      "version": "6.3.1"
    },
    "tns-ios": {
      "version": "6.3.0"
    }
  },
  "scripts": {
    "tsc": "cd app && tsc --skipLibCheck --watch",
    "ios-and-android": "tns run --bundle",
    "ios": "tns run ios --bundle",
    "ios.release": "tns build ios --bundle --release --forDevice --provision 56140ef2-ea59-483c-8fe9-8438fc8a892c",
    "ios.emulator": "tns run ios --bundle --emulator",
    "android": "tns run android --bundle",
    "android.release": "tns build android --bundle --release --for-device --key-store-path ./keystore-footplr.jks --key-store-password xs4all --key-store-alias footplr --key-store-alias-password xs4all",
    "colin": "tns run ios --bundle --device 'iPhone 11 Pro"
  },
  "dependencies": {
    "@nativescript/theme": "~2.2.1",
    "@proplugins/nativescript-purchase": "^4.0.0",
    "@vue/devtools": "^5.3.3",
    "moment": "^2.24.0",
    "nativescript-advanced-permissions": "^1.2.0",
    "nativescript-appversion": "^1.4.4",
    "nativescript-camera": "^4.5.0",
    "nativescript-datetimepicker": "^1.2.2",
    "nativescript-email": "^1.5.5",
    "nativescript-imagecropper": "^3.0.0",
    "nativescript-imagepicker": "^7.1.0",
    "nativescript-inappbrowser": "^2.2.0",
    "nativescript-iqkeyboardmanager": "^1.5.1",
    "nativescript-open-app": "^0.3.0",
    "nativescript-plugin-firebase": "^10.3.3",
    "nativescript-rater": "^2.1.1",
    "nativescript-social-share": "^1.6.0",
    "nativescript-socketio": "^3.3.1",
    "nativescript-toasty": "^3.0.0-alpha.2",
    "nativescript-ui-listview": "^8.0.1",
    "nativescript-vue": "^v2.5.0-alpha.3",
    "nativescript-vue-devtools": "^1.2.0",
    "nativescript-vue-shadow": "^0.1.0",
    "tns-core-modules": "^6.3.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "~7.1.0",
    "@babel/preset-env": "~7.1.0",
    "babel-loader": "~8.0.0",
    "nativescript-dev-webpack": "~1.4.0",
    "nativescript-vue-template-compiler": "~2.4.0",
    "node-sass": "^4.7.1",
    "vue-loader": "~15.4.0"
  },
  "description": "Store your holiday cards forever, without taking up any space on the fridge.",
  "author": "Fitz Creative"
}
